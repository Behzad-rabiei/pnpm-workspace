{
      "name": "pnpm-explore",
      "version": "1.0.0",
      "description": "",
      "main": "index.js",
      "workspaces": [
            "packages/*",
            "apps/*"
      ],
      "scripts": {
            "dev": "pnpm -r --parallel dev",
            "start": "pnpm -r start",
            "build": "pnpm -r build",
            "check": "pnpm -r check",
            "lint": "pnpm -r lint",
            "format": "pnpm exec biome format --write",
            "changeset": "changeset",
            "start:backend": "pnpm --filter @my/backend dev",
            "start:frontend": "pnpm --filter @my/frontend dev",
            "start:temporal": "pnpm --filter @my/temporal-workflows dev",
            "test": "vitest",
            "test:ui": "vitest --ui"
      },
      "devDependencies": {
            "@biomejs/biome": "^2.0.6",
            "@changesets/cli": "^2.29.5",
            "@swc/core": "^1.12.7",
            "@vitejs/plugin-react": "^4.5.2",
            "@vitest/coverage-v8": "^3.2.4",
            "@vitest/ui": "^3.2.4",
            "tsconfig-paths": "^4.2.0",
            "typescript": "^5.5.0",
            "unplugin-swc": "^1.5.5",
            "vite-tsconfig-paths": "^5.1.4",
            "vitest": "^3.2.4"
      },
      "keywords": [],
      "author": "",
      "license": "ISC",
      "packageManager": "pnpm@10.12.4"
}
